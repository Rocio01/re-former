<%= form_for(@user, url: user_path(@user), html:{multipart: true, method: "patch"}) do |f| %>
  <%= f.label :username, 'Name' %>
  <%= f.text_field :username, value: 'Jhon Doe'%> <br><br>
  <%= f.label :email %>
  <%= f.email_field :email,  placeholder: 'example@email.com' %> <br><br>
  <%= f.label :password %>
  <%= f.password_field :password %> <br><br>
  <%= f.submit %>
<% end%>
